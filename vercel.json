{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@5.0.0"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/main.py" },
    { "source": "/(.*)", "destination": "/api/main.py" }
  ]
}
