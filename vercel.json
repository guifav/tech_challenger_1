{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/main.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/main.py"
    }
  ]
}